    "push:stg_de": "rm -rf build_tmp && cp -r build build_tmp && rm -rf build_tmp/royal build_tmp/static && scp -P 22 -r build_tmp/* stgd2capp@52.188.61.82:/data/www/s2b/shop/dist_shop_de && rm -rf build_tmp",
